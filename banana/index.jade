doctype html
//- basic libraries: jquery(1.10.2), d3js(3.3.11), angularjs(1.2.3), bootstrap(3.0)/semantic-ui(0.9.3/0.9.5)
- var usecdn = false
- var useog = false
- var lib = { jquery: true, d3js: true, angular: true, bootstrap: false, semantic: true }
- var assets = "assets"
- var thumbnail = "http://zbryikt.github.io/visualize/banana/thumbnail.png"
html(lang="en")
  head(prefix="og: http://ogp.me/ns#")
    meta(charset="utf-8")
    title template
    // libraries
    include library.jade
    // custom files
    link(rel="stylesheet",type="text/css",href="x3dom.css")
    link(rel="stylesheet",type="text/css",href="index.css")
    script(type="text/javascript",src="x3dom.js")
    script(type="text/javascript",src="fisheye.js")
    script(type="text/javascript",src="banana.js")
    script(type="text/javascript",src="index.js")
  body(ng-app,ng-controller="main")
    h2 Banana 3D - Angularjs + D3js fisheye plugin + X3Dom
    x3d(profile="Immersive",version="3.1",xsd:noNamespaceSchemaLocation="http://www.web3d.org/specifications/x3d-3.1.xsd",xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance",onmousemove="movehandle(event)")
      head
        meta(content="banana.x3d",name="title")
        meta(content="Generated from Meshlab X3D Exported",name="description")
        meta(content="11 4æœˆ 2014",name="created")
        meta(content="Meshlab X3D Exported, http://meshlab.sourceforge.net",name="generator")
      Scene
        viewpoint(position="-70 -70 400",fieldofview="0.785398",orientation="0,0,0,0",
        centerofrotation="-70,-70,0",znear="-1",zfar="-1")
        Shape
          appearance(sorttype="auto")
            material(diffusecolor="0.903 0.994 0.309",ambientintensity="0.2",
            emissivecolor="0,0,0",shininess="0.9",specularcolor="1,1,0")
          IndexedFaceSet(ng-attr-coordIndex="{{coordindex}}",solid="false",normalPerVertex="false")
            Coordinate(ng-attr-point="{{point}}")
            //Normal(ng-attr-vector="{{vector}}")


